{
  "hosting": {
    "public": "coffee-horizon-frontend/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/images/**",
        "destination": "/images/"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
